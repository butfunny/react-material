.date-picker-list-years {
  height 100%;
  overflow-y: auto;
  text-align center;
  font-size 16px;
  margin 20px 0;
  -webkit-overflow-scrolling touch


  div {
    padding 5px;
    cursor pointer;

    &:hover {
      color #008B7D;
    }

    &.selected {
      color #008B7D !important;
      font-size 26px;
    }
  }
}