.date-picker {
  .modal-box {
    width 320px !important;
  }

  .btn-close {
    display none;
  }

}


.date-picker-popup {
  width: 320px;
  box-shadow: rgba(0, 0, 0, 0.25) 0px 14px 45px, rgba(0, 0, 0, 0.22) 0px 10px 18px;
  border-radius 5px;

  .date-picker-body {
    height: 320px;
    padding 0 10px;
  }

  .date-picker-footer {
    text-align right;
    padding 10px 10px;
    .btn {
      color #008B7D;
      font-size 15px;
      font-weight: 600;

      .btn-elem {
        padding: 0 10px;
      }
    }
  }
}